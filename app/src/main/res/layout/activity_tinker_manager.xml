<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tikeyc.ttinkerpatchdemo.classes.tinkerManager.TinkerManagerActivity">

    <Button
        android:id="@+id/onReceiveUpgradePatch_button"
        android:text="请求打补丁"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/cleanPatch_button"
        android:text="卸载补丁"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/killAllOtherProcess_button"
        android:text="杀死应用的其他进程"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <Button
        android:id="@+id/installNavitveLibraryABI_button"
        android:text="Hack方式修复so"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <Button
        android:id="@+id/loadLibraryFromTinker_button"
        android:text="非Hack方式修复so"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <Button
        android:id="@+id/login_button"
        android:text="登录"
        android:background="@android:color/holo_green_dark"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>
